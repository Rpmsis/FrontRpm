 <v-dialog v-model="alerta" width="500">
          <v-card>
              <v-card-title class="text-h4">
                  {{ Titulo }}
              </v-card-title>

              <v-card-text class="text-h6 text-center">

                  {{ Mensaje }}
              </v-card-text>
              <v-divider></v-divider>

              <v-card-actions>
                  <v-spacer></v-spacer>
                  <v-btn color="primary" text @click="alerta = false">
                      Cerrar
                  </v-btn>
              </v-card-actions>
          </v-card>

      </v-dialog>


 data() {
      return {
          alerta: false,
           Mensaje: "",
            Titulo: "",
}
}
